{
	"name": "PL_CRYPTO_REST_API",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "CurrenyNameList",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Destination",
						"value": "synapsestoragefss/RawSource/"
					}
				],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['price']"
								},
								"sink": {
									"name": "price"
								}
							},
							{
								"source": {
									"path": "$['exchange']"
								},
								"sink": {
									"name": "exchange"
								}
							},
							{
								"source": {
									"path": "$['pair']"
								},
								"sink": {
									"name": "pair"
								}
							},
							{
								"source": {
									"path": "$['pairPrice']"
								},
								"sink": {
									"name": "pairPrice"
								}
							},
							{
								"source": {
									"path": "$['volume']"
								},
								"sink": {
									"name": "volume"
								}
							}
						],
						"collectionReference": ""
					}
				},
				"inputs": [
					{
						"referenceName": "DS_CRYPTO_REST_API",
						"type": "DatasetReference",
						"parameters": {
							"currencyname": "bitcoin"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_CryptoCurrency_List",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "CurrenyNameList",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"fileListPath": "synapsestoragefss/RawSource/CryptoCurrency_List.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"dataset": {
						"referenceName": "DS_CryptoCurrency_List",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			}
		],
		"annotations": []
	}
}