										},
										"sink": {
											"name": "O3",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "CO",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "CO",
											"type": "String",
											"physicalType": "UTF8"
										}
									},
									{
										"source": {
											"name": "SO2",
