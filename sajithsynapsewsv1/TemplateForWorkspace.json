					},
					{
						"name": "NO2",
						"type": "String"
					},
					{
						"name": "AQI_Class",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('workspaceId'), '/linkedServices/LS_synapse_ws_ADLS')]"
			]
		},
		{
			"name": "[concat(parameters('workspaceName'), '/DS_Pollution_Parquet')]",
			"type": "Microsoft.Synapse/workspaces/datasets",
			"apiVersion": "2019-06-01-preview",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LS_synapse_ws_ADLS",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"folderPath": "RawParquet",
						"fileSystem": "synapsestoragefss"
