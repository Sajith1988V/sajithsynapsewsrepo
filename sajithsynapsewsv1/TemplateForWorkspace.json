					"referenceName": "LS_synapse_ws_ADLS",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "IND_and_Nep_AQI_Dataset.csv",
						"folderPath": "RawSource/Pollution",
						"fileSystem": "synapsestoragefss"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Location",
						"type": "String"
					},
					{
